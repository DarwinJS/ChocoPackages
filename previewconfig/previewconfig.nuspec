    <copyright>Ramesh Srinivasan</copyright>
    <iconUrl>https://cdn.rawgit.com/DarwinJS/ChocoPackages/master/previewconfig/appicon.png</iconUrl>
    <releaseNotes></releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
