<Configuration>
  <ViewDefinitions>
    <View>
      <Name>CurrentUserTable</Name>
      <ViewSelectedBy>
        <TypeName>PSPrivilege.CurrentUserPrivilege</TypeName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>Privilege</Label>
            <Width>32</Width>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Description</Label>
            <Width>40</Width>
