          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Privilege</Label>
            <Width>32</Width>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Accounts</Label>
            <Width>28</Width>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>Computername</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Privilege</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Accounts</PropertyName>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
      </TableControl>
    </View>
    <View>
      <Name>CurrentUserList</Name>
      <ViewSelectedBy>
        <TypeName>PSPrivilege.CurrentUserPrivilege</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>Account</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Privilege</PropertyName>
