              <ListItem>
                <PropertyName>Computername</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
    <View>
      <Name>MainPrivList</Name>
      <ViewSelectedBy>
        <TypeName>PSPrivilege.Privilege</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>Computername</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Privilege</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Description</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Accounts</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
